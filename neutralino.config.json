{
  "$schema": "https://neutralino.js.org/schemas/neutralino.config.schema.json",
  "applicationId": "js.neutralino.studybub",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "url": "/resources/index.html",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "storage.*",
    "window.*"
  ],
  "modes": {
    "window": {
      "title": "bruno",
      "width": 1400,
      "height": 900,
      "minWidth": 800,
      "minHeight": 600,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/resources/icons/luxury_letter_a_logo_design_for_business_or_online_shop-removebg-preview.png",
      "enableInspector": true,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": true
    }
  },
  "cli": {
    "binaryName": "bruno",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "4.10.0",
    "clientVersion": "3.8.0"
  }
}